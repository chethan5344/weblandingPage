{
  "manifest_version": 3,
  "name": "Animation Extension",
  "version": "1.0",
  "description": "A Chrome extension that runs an animation when Chrome loads.",
  "background": {
    "scripts": ["script.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab"
  ]
}
